@mixin rounded($vert, $horz, $radius: 10px) {
    border-#{$vert}-#{$horz}-radius: $radius;
    -moz-border-radius-#{$vert}#{$horz}: $radius;
    -webkit-border-#{$vert}-#{$horz}-radius: $radius;
}

@mixin rounded-top-left {
    $vert: top;
    $horz: left;
    $radius: 10px;

    border-#{$vert}-#{$horz}-radius: $radius;
    -moz-border-radius-#{$vert}#{$horz}: $radius;
    -webkit-border-#{$vert}-#{$horz}-radius: $radius;
}


// --- HTML5 CROSSBROWSER PATCHES --- //
@mixin border_radius($radius: 2px) {
    border-radius: $radius;
}

@mixin box-shadow($values){
    -webkit-box-shadow: $values;
    box-shadow: $values;
}



body{
    @include box_settings($orient: vertical, $align: start, $pack: center, $flex: 0);

    a {
        /*REMOVE THE DASHED OUTLINE OF LINKED OBJECTS*/
        outline: 0;
        text-decoration: none; /*REMOVE THE DASHED OUTLINE OF LINKED OBJECTS*/

        /*REMOVE BORDERS AROUND IMAGE LINKS*/
        &[href$=jpg], &[href$=jpeg], &[href$=jpe], &[href$=png], &[href$=gif], &[href$=tiff] {
            text-decoration: none;
            border: 0 none;
        }
    }
}

// MY BASIC HTML PAGE SETUP -- WORKING FROM LARGEST TO SMALLEST BLOCKS
@mixin basic_setup() {

        header, section, footer, aside, nav, article, figure  {
                display: block;
        }

        body {
                width: 100%;
                font-size: 62.5%; /* Base font size: 10px */
                line-height: 1.2;
                text-align: justify;
                clear: both;
                margin: 0 auto;
        }

        // RESETS
        * {
            margin: 0px;
            padding: 0px;
        }

        // HEADINGS
        h2 {font-size: 2.2em}   /* 22px */
        h3 {font-size: 2.0em}   /* 20px */
        h4 {font-size: 1.8em}   /* 18px */
        h5 {font-size: 1.6em}   /* 16px */
        h6 {font-size: 1.4em}   /* 14px */

        h2, h3, h4, h5, h6 {
            font-weight: 400;
            line-height: 1.1;
            margin-bottom: .8em;
            text-align: left;
        }

        // DIV LAYOUT BLOCKS
        div.divVerticalKids{
            @include box_settings($orient: vertical, $align: start, $pack: center, $flex: 0);
        }

        div.divHorizontalKids{
            @include box_settings($orient: horizontal, $align: center, $pack: center, $flex: 0);
        }

        // PARAGRAPHS
        p {
            margin-bottom: 1.143em;
        }
        * p:last-child {
            margin-bottom: 0;
        }

        // IMAGES
        img.right,figure.right {
                float: right;
                margin: 0 0 10px 10px;
        }

        img.left,figure.left {
                float: left;
                margin: 10px 30px 0 0;
        }

        // TABLES
        table {
                margin: {
                        left: auto;
                        right: auto;
                        bottom: 24px;
                }

                border: {
                        bottom: 1px solid;
                        right: 1px solid;
                        spacing: 0;
                }

                th {
                        padding: 3px 10px;
                        font-size: $baseFontSize;
                        border: {
                                top: 1px solid;
                                left: 1px solid;
                        }
                }
                td {
                        padding: 3px 10px;
                        font-size: $baseFontSize*0.85;
                        border: {
                                top: 1px solid;
                                left: 1px solid;
                        }
                }
        }

        form {
            background: $solGrey00;
            color:$solGrey-01;
            margin: 0px;
            padding: 5px;

            -webkit-background-clip: padding-box; /* fix WebKit background bleed on border-radius */
            border-radius: 5px 5px 5px 5px;

            span {
                color: $solGrey00;
                position: absolute;
                top:4px;
                right:14px;
                text-shadow: 1px 1px 1px rgba(0, 0, 0, .1);
            }

            fieldset {
                background: $solGrey00;
                border: none;
            }

            legend {
                font-size: 1.25em;
                margin-bottom: 3px;
                padding: 0 1px;
                color: $solGrey-01;
                font-size: 1.2em;
                font-weight: bold;
                margin-bottom:5px;
            }

            label {font-size: 1.2em;}

    /* Input Types */
    input{

        &[type='text'], &[type='email'], &[type='url'], &[type='date'] {
            height:24px;
            background: $solGrey-01;
            border: 1px solid $solGrey02;
            color: $solGrey00;
            font-family: inherit;
            font-size: inherit;
            padding:4px;
            margin:2px 10px;
            font-style: italic;
            vertical-align: center;

            @include box-shadow(inset 0px 0px 2px rgba(0, 0, 0, .4));

            border-radius: 2px;

            /* useful if you don't want a bg color from leaking outside the border: */
            -webkit-background-clip: padding-box;
            -moz-background-clip: padding-box;
            background-clip: padding-box;
        }
    }

    textarea {

    }
    input[type='text']:hover,
    input[type='email']:hover,
    input[type='url']:hover,
    textarea:hover {
        background: $white;
        border-color: $solGrey03;
        cursor: text;
    }
    input[type='text']:focus,
    input[type='email']:focus,
    input[type='url']:focus,
    textarea:focus {
        background: $white;
        border-color: $solGrey03;
        color: $solGrey05;
        font-style: normal;
    }

    input[type='checkbox'], input[type='radio'] {
        display: block;
        margin-top: 4px;
        margin-left: 10px;
    }

    input[type='submit'] {
        background: $salmon;
        border: 0;
        border-radius: 5px;
        color: $white;
        cursor: pointer;
        font-family: inherit;
        font-size: inherit;
        padding: .3em 2em;
        text-shadow: 1px 1px 1px #000;
    }

    select{
        background: transparent;
        border: 1px solid #CCC;
        margin: 2px 10px;

        display: -webkit-box;
        display: -moz-box;
        display: box;

        -webkit-background-clip: padding-box; /* fix WebKit background bleed on border-radius */
        -webkit-border-radius: 2px;
           -moz-border-radius: 2px;
                border-radius: 2px;

        -webkit-appearance: none;

        width: 150px;
        padding-left: 9px;

        overflow: hidden;
        background: url(/pessto/images/down_arrow_select.jpg) no-repeat right #DDD;
        font-size: 14px;
        line-height: 20px;
        color: #444;
        text-shadow: white 0 1px 0;
        height:24px;
    }
        }

        // BUTTONS
        button[class*="Button"] {
            @include box_settings($orient: horizontal, $align: center, $pack: center, $flex: 0);
            background: $solGrey-01;
            border: none;

            -webkit-background-clip: padding-box; /* fix WebKit background bleed on border-radius */
            border-radius: 2px;

            color: $solGrey03;
            font-size: 1.2em;
            line-height:0.9;
            padding: 7px 10px;
            margin: 2px 5px 2px 5px;

            &:hover {
                background: $solGrey03;
                color:$solGrey-01;
                cursor: pointer;
            }

            &.redButton {
                background: $solRed;
                color:$solGrey-01;
                &:hover {
                    background: lighten($solRed, 10%)
                }
            }

            &.blueButton {
                background: $solBlue;
                color:$solGrey-01;
                &:hover {
                    background: lighten($solBlue, 10%);
                }
            }

            &.clearButton {
                background: none;
                color:$solGrey-01;
                &:hover {
                    color: $solBlue;
                    background: none;
                }
            }
        }

        // TEXT
        strong,b {
            font-weight: bold;
        }

        em,i {
            font-style: italic;
        }

        // QUOTES
        blockquote {
            font-style: italic;
        }

        // ANCHORS
        a {
            outline: 0;
            text-decoration: none;
            padding: 0 1px;
            border-bottom:1px dashed;

            &[href$=jpg], &[href$=jpeg], &[href$=jpe], &[href$=png], &[href$=gif], &[href$=tiff] {
                border: 0 none;
            }
        }

        //DEFINITION
        dl {
            margin: 0 0 1.5em 0;
        }

        dt {
            font-weight: bold;
        }

        dd {
            margin-left: 1.5em;
        }
}

