@import "global_forms";

@mixin box_settings($orient, $align: start, $pack: start, $flex: 0, $flexValue: 1){
        display: -webkit-box; display: -moz-box; display: box;
        -webkit-box-orient: $orient;
        -moz-box-orient: $orient;
        -ms-box-orient: $orient;
        box-orient: $orient;
        -webkit-box-align: $align;
        -moz-box-align: $align;
        -ms-box-align: $align;
        box-align: $align;
        -webkit-box-pack: $pack;
        -moz-box-pack: $pack;
        -ms-box-pack: $pack;
        box-pack: $pack;

        @if $flex == 1{
            -webkit-box-flex: $flexValue;
            -moz-box-flex: $flexValue;
            -ms-box-flex: $flexValue;
            box-flex: $flexValue;
        }
}

@mixin transition_all($secs: 1s){
    -webkit-transition: all $secs ease;
    -moz-transition: all $secs ease;
    -o-transition: all $secs ease;
    -ms-transition: all $secs ease;
    transition: all $secs ease;
}

// ----------------------------------
// BASIC COLORS
// ----------------------------------
$white: #ffffff;
$black: #000000;
$darkBrown: #42231c;
$lightCream: #fbf8ea;
$lightMud: #807227;
$brickRed: #7a3a2c;
$aquaMarine: #60b0a1;
$biscuit: #e4dfc6;
$black: #000305;
$salmon: #C74350;
$cream: #E7E2C5;
$purple: #B26FAE;
$blue: #4f8fc1;
$green: #4a7c42;


// ----------------------------------
// SOLARIZED COLOR SCHEME
// ----------------------------------
$solGrey05:    #002b36;
$solGrey04:    #073642;
$solGrey03:    #586e75;
$solGrey02:    #657b83;
$solGrey01:     #839496;
$solGrey00:     #93a1a1;
$solGrey-01: #D2D1D1;
$solCream01:     #eee8d5;
$solCream00:     #fdf6e3;
$solYellow:    #b58900;
$solOrange:    #cb4b16;
$solRed:       #dc322f;
$solMagenta:   #d33682;
$solViolet:    #6c71c4;
$solBlue:      #268bd2;
$solCyan:      #2aa198;
$solGreen:     #859900;
// FLIP BASE COLORS
@mixin rebase($rebase03,$rebase02,$rebase01,$rebase00,$rebase0,$rebase1,$rebase2,$rebase3)
{
        background-color:$rebase03;
        color:$rebase0;
        * { color:$rebase0; }
        h1,h2,h3,h4,h5,h6 { color:$rebase1; border-color: $rebase0; }
        a, a:active, a:visited { color: $rebase1; }
}
// ADD HEADER AND LINK COLORS
@mixin accentize($accent) {
        a, a:active, a:visited, code.url { color: $accent; }
        h1,h2,h3,h4,h5,h6 {color:$accent}
}

